  <% posts.posts.forEach(post => { %>

     <div onclick="window.location.href='/post/<%=post.id%>'" class="container" style="padding-top: 40px; border: 1px black; cursor: pointer;">
        <div class="row">
          <div class="col-md-5">
            <img src="<%= post.image %>" id="img-fluid" class="img-fluid" style="height: 300px; width: 400px;" alt="">
          </div>

            <div class="col-md-5 text-center h-80 text-center d-flex align-self-center d-flex flex-column">
            <p style="font-size: 30px; font-weight: bold;"><%= 
 post.title %></p>
            <p id="desc" style=" font-family: 'PT Serif', serif; font-size: 20px;"> 
                <%= post.description %></p>
          </div>
        </div>
      </div>

    <% }) %>
<ul class="pagination">
 <% if (page <= 0) { %>
    <li class="page-item disabled"><a class="page-link" href="#">NEWER POSTS</a></li>
   <% } else { %>
     <li class="page-item"><a class="page-link" href="https://lifewithvetta.com/blog?page=<%=page-1 %>">NEWER POSTS</a></li>
     <% } %>
       
 <% if (page >= maxpage) { %>
    <li class="page-item disabled"><a class="page-link" href="#">OLDER POSTS</a></li>
   <% } else { %>
     <li class="page-item"><a class="page-link" href="https://lifewithvetta.com/blog?page=<%=page+1 %>">OLDER POSTS</a></li>
     <% } %>
</ul>
  
  <script defer>
    
    console.log("<%= page %>")
         function decodeHtml(html) {
    var txt = document.createElement("textarea");
    txt.innerHTML = html;
    return txt.value;
}
      var stringToHTML = function (str) {
	var dom = document.createElement('div');
	dom.innerHTML = str;
	return dom;
};   

    let posts = [...document.querySelectorAll("#desc")]

    posts.forEach(post => {
      const html = decodeHtml(post.innerHTML.slice(0, 500) + "")
         const htmlPlace = stringToHTML(html)
      post.textContent = ""
    post.appendChild(htmlPlace)
                  

    })
    
  </script>

  

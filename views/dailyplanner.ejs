  <script>
    function addToCart() {
      let cartTemplate = {
        name: "Monthly Planner 2023",
        id: "monthlyplanner2023",
        image: "/images/planner_thumb.png",
        size: "None",
        price: 4.99
      }
      
      let storage = sessionStorage.getItem("cart")
      let newStorage

      console.log(storage)
       if (storage == "" || storage == null) {
          sessionStorage.setItem('cart', JSON.stringify([]))
         newStorage = JSON.parse(sessionStorage.getItem("cart"))
      } else {
         newStorage = JSON.parse(storage)
      }
      
      
      newStorage.push(cartTemplate)
      sessionStorage.setItem('cart', JSON.stringify(newStorage))
      alert("Successfully added to cart!")
    }

     <script>
  function sizeChanged(e) {
    let wasPrice = document.getElementById("wasprice")
    let priceInString = wasPrice.textContent.slice(5, 1000000);
    let priceInString2 = priceInString.replace('$', '')
      <% for(let size of shirt.Sizes) {  %>
    if ("<%= size[Object.keys(size)[0]].Name %>" == e.target.value && "<%= size[Object.keys(size)[0]].Price %>" !== "FromPrice") {
      wasPrice.textContent = "$"
        wasPrice.textContent += parseFloat(<%= shirt.FromPrice %>) + parseFloat(<%= size[Object.keys(size)[0]].Price %>)
    } else if ("<%= size[Object.keys(size)[0]].Name %>" == e.target.value && "<%= size[Object.keys(size)[0]].Price %>" == "FromPrice") {
      wasPrice.textContent = `$${<%= shirt.FromPrice %>}`
    }
    <% } %>
}
  function addToCart() {
    let wasPrice = document.getElementById("wasprice")
    let sizes = document.getElementById("shirtForm")
    let colors = document.getElementById("shirtForm2")
    let price
    if (wasPrice.textContent.includes("From")) {
      price = parseFloat(wasPrice.textContent.replace("From $", ''))
    } else {
      price = parseFloat(wasPrice.textContent.replace("$", ''))
    }
    let cartTemplate = {
        name: `<%= shirt.Name %> (${colors.value})`,
        id: "<%= shirt.Name.toLowerCase().replace(' ', "-") %>",
        image: "<%= shirt.MainIMG %>",
        size: sizes.value,
        color: colors.value,
        price: price,
    }

    let storage = sessionStorage.getItem("cart")
    let newStorage
    console
        .log(storage)
        if (storage == "" || storage == null) {
            sessionStorage.setItem('cart', JSON.stringify([]))
            newStorage = JSON.parse(sessionStorage.getItem("cart"))
        } else {
            newStorage = JSON.parse(storage)
        } newStorage
        .push(cartTemplate)
        sessionStorage
        .setItem('cart', JSON.stringify(newStorage))
}
       
      
</script> 
